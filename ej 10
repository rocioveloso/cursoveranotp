#include<iostream>

using namespace std;

int main(){
	int a, b, c;
	cout<<"Ingrese los lados del triangulo: "<<endl;
	cin>>a>>b>>c;
	
	if(a==b&&a==c){
		cout<<"Es equilatero."<<endl;
	}else{
		if(a==b||a==c||c==b){
			cout<<"Es isosceles."<<endl;
		}else{
			cout<<"Es escaleno."<<endl;
		}
		
	}
		
	return 0;
}
