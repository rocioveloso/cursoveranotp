#include<iostream>

using namespace std;

int main(){
	int vec[50];
	int c=0, sum=0, sump=0;
	int prom;
		
	for(int i=0; i<50; i++){
		if(vec[i]>100){
			c++;
			sump=sump+vec[i];
		}
		if(vec[i]<-10){
			sum=sum+vec[i];
		}
	}
	prom=sump/c;
	cout<<"Promedio de los mayores de 100: "<<prom<<endl;
	cout<<"Sumatoria de los menores que -10: "<<sum<<endl;
		
	return 0;
}
