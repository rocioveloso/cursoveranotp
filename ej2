#include <iostream>
using namespace std;

int main() {
	int a, b;
	cout<<"Ingrese dos numeros: ";
	cin>>a>>b;
	cout<<endl;
	
	cout<<"La suma de ambos es: "<<a+b<<endl;
	cout<<"La resta de ambos es: "<<a-b<<endl;
	cout<<"La multiplicacion de ambos es: "<<a*b<<endl;
	cout<<"La division entre ambos es: "<<a/b<<endl;
	
	return 0;
}
